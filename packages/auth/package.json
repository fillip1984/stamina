    "next": "^16.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.7",
    "@stamina/prettier-config": "workspace:*",
    "@stamina/tsconfig": "workspace:*",
    "@types/react": "^19.2.7",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "prettier": "@stamina/prettier-config"
