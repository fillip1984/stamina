{
  "name": "@stamina/db",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "type": "module",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "default": "./src/schema.ts"
    },
    "./enums": {
      "types": "./dist/enums.d.ts",
      "default": "./src/enums.ts"
    },
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./src/dbEnv.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc",
    "format": "prettier --write . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "generate": "pnpm with-env drizzle-kit generate",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "push": "pnpm with-env drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^3.0.6",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.8",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@stamina/prettier-config": "workspace:*",
    "@stamina/tsconfig": "workspace:*",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "prettier": "@stamina/prettier-config"
}
